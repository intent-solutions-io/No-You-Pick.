# NoYouPick Cloud Functions Environment Variables
# Copy this file to .env and fill in your values

# Gemini API Key (for local development only)
# In production, use Firebase Secret Manager:
#   firebase functions:secrets:set GEMINI_API_KEY
GEMINI_API_KEY=your-gemini-api-key-here

# Firebase Project Configuration
# These are automatically set by Firebase in production
GCLOUD_PROJECT=noupick-staging
FIREBASE_CONFIG={"projectId":"noupick-staging"}
